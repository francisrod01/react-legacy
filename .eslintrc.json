{
    "extends": "airbnb",
    "plugins": [
        "import",
        "jsx-a11y",
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "function-paren-newline": 0,
        "no-multi-str": "error",
        "jsx-a11y/anchor-is-valid": ["error", {
            "components": ["link"],
            "specialLink": ["to"]
        }]
    }
}